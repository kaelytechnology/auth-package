{
    "name": "kaely/auth-package",
    "description": "Paquete de autenticaci√≥n para Laravel con roles y permisos",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kaely Development Team",
            "email": "dev@kaely.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kaely\\AuthPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaely\\AuthPackage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaely\\AuthPackage\\AuthPackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 